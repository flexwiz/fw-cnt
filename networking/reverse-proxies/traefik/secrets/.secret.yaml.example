# traefik-tls-secret.yaml
apiVersion: v1
kind: Secret
metadata:
  name: traefik-tls
  namespace: traefik
type: kubernetes.io/tls
data:
  # These are example base64-encoded values
  # In a real scenario, replace these with your actual certificate and private key
  # You can generate these values using:
  # cat tls.crt | base64 -w 0
  # cat tls.key | base64 -w 0
  tls.crt: 
  tls.key: 
